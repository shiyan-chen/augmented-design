{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@antv/data-set": "0.10.2",
    "antd": "3.19.6",
    "axios": "0.19.0",
    "bizcharts": "3.5.4",
    "c3": "0.7.1",
    "chart.js": "2.8.0",
    "d3": "5.9.2",
    "echarts": "4.2.1",
    "echarts-gl": "1.1.1",
    "eslint-config-airbnb": "^18.0.1",
    "js-yaml": "3.13.1",
    "leaflet": "^1.5.1",
    "moment": "2.24.0",
    "node-sass": "^4.12.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-d3-cloud": "0.7.0",
    "react-dom": "16.8.6",
    "react-leaflet": "^2.4.0",
    "react-redux": "7.1.0",
    "react-router-dom": "4.3.1",
    "react-scripts": "3.0.1",
    "react-svg": "10.0.9",
    "redux": "4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf ../backend/public && react-scripts build && mv build ../backend/public",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "implicit-arrow-linebreak": "off",
      "comma-dangle": "off",
      "indent": "off",
      "no-trailing-spaces": "off",
      "import/extensions": "off",
      "import/no-named-as-default-member": "off",
      "react/jsx-curly-newline": "off",
      "react/state-in-constructor": "off",
      "react/static-property-placement": "off",
      "react/jsx-props-no-spreading": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
